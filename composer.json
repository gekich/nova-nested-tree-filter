{
    "name": "gekich/nested-tree-filter",
    "description": "A Laravel Nova nested tree filter.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "kalnoy/nestedset": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Gekich\\NestedTreeFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gekich\\NestedTreeFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
